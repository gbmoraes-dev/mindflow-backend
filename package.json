{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "module": "src/http/server.ts",
  "scripts": {
    "dev": "bun run --watch src/http/server.ts",
    "worker": "bun run --watch src/worker/subscriber.ts",
    "build": "bun build --compile --external pg --minify-whitespace --minify-syntax --target bun --outfile server src/http/server.ts",
    "start": "./server",
    "pretest": "bun --env-file .env.test --bun drizzle-kit migrate",
    "test": "bun test --env-file .env.test",
    "format": "biome format --write src/",
    "lint": "biome lint src/",
    "check": "biome check --write src/",
    "typecheck": "tsc",
    "db:generate": "bun --env-file .env --bun drizzle-kit generate",
    "db:migrate": "bun --env-file .env --bun drizzle-kit migrate",
    "db:seed": "bun --env-file .env --bun drizzle-kit src/db/seed.ts",
    "db:studio": "bun --env-file .env --bun drizzle-kit studio"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.64",
    "@elysiajs/cors": "1.4.0",
    "@elysiajs/openapi": "1.4.11",
    "@elysiajs/opentelemetry": "1.4.6",
    "@opentelemetry/exporter-trace-otlp-proto": "0.207.0",
    "@opentelemetry/instrumentation-pg": "0.60.0",
    "@opentelemetry/sdk-trace-node": "2.2.0",
    "ai": "5.0.89",
    "amqplib": "0.10.9",
    "better-auth": "1.3.34",
    "dayjs": "1.11.19",
    "drizzle-orm": "0.44.7",
    "drizzle-zod": "0.8.3",
    "elysia": "1.4.15",
    "neverthrow": "8.2.0",
    "pg": "8.16.3",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.3",
    "@types/amqplib": "0.10.8",
    "@types/pg": "8.15.6",
    "bun-types": "1.3.1",
    "drizzle-kit": "0.31.6"
  }
}
